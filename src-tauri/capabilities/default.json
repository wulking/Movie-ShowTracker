{
  "identifier": "default",
  "description": "Default permissions for the app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://api.themoviedb.org/**" }
      ]
    }
  ]
}
